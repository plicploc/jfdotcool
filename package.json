  "scripts": {
    "dev": "node esbuild.config.mjs --watch",
    "build": "node ./scripts/build.mjs --minify",
    "preversion": "node ./scripts/ensure-clean.mjs",
    "version": "npm run build && git add -A dist && git commit -m \"build: dist for v$npm_package_version\"",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch -m \"chore(release): v%s\"",
    "release:minor": "npm version minor -m \"chore(release): v%s\"",
    "release:major": "npm version major -m \"chore(release): v%s\""
  },
